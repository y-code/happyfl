{
    "urls": "http://localhost:5000;https://localhost:5050/",
  "ConnectionStrings": {
      "happyfl-db": "Server=localhost;Port=4321;User Id=postgres;Password=postgres;Database=happyfl;pooling=true;maxpoolsize=30;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "HappyFL"
    }
  },
  "AllowedHosts": "*"
}
